  "main": "dist/index.js",
  "directories": {
    "test": "tests"
  },
  "scripts": {
    "build": "babel src --out-dir dist --extensions \".ts,.tsx\"",
    "dev": "concurrently \"babel src --out-dir dist --extensions \".ts,.tsx\" --watch\" \"nodemon -q dist/index.js\"",
    "lint": "eslint src",
    "lint:fix": "eslint src --fix",
    "prepare": "husky install",
    "prisma-db-pull": "yarn prisma db pull",
    "prisma-generate": "yarn prisma generate",
    "sort-package": "yarn sort-package-json",
    "test": "jest --config jest.config.ts --coverage",
    "tree": "git ls-tree -r --name-only HEAD | tree --fromfile"
  },
  "lint-staged": {
    "src/**/*.{ts,tsx}": [
      "prettier --write",
      "eslint --fix"
    ]
  },
  "dependencies": {
    "@prisma/client": "latest",
    "bcrypt": "^5.1.1",
